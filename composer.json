{
    "name": "ronash/yii2-vendor-management",
    "description": "Flexible vendor registration and authentication module for lunch hub project",
    "keywords": ["yii2", "lunch hub", "vendor management"],
    "type": "yii2-extension",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/dektrium/yii2-user/issues?state=open",
        "forum": "https://gitter.im/dektrium/yii2-user",
        "source": "https://github.com/dektrium/yii2-user"
    },
    "authors": [
        {
            "name": "Ronash Dhakal",
            "email": "ronashmail@gmail.com",
            "homepage": "http://www.ronash.com.np"
        }
    ],
    "prefer-stable": true,
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "^2.0.13",
        "yiisoft/yii2-swiftmailer": "^2.0.0",
        "yiisoft/yii2-authclient": "^2.1.0",
        "yiisoft/yii2-bootstrap": "^2.0.0"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "^2.0.0",
        "codeception/specify": "^0.4.3",
        "codeception/verify": "^0.3.1"
    },
    "autoload": {
        "psr-4": {
            "ronash\\vendor\\": ""
        }
    },
    "extra": {
        "bootstrap": "ronash\\vendor\\Bootstrap",
        "branch-alias": {
            "dev-master": "1.0-dev"

        }
    }
}
